#include<stdio.h>
main() {
 int score;
 printf("성적 입력 : ");
 scanf("%d", &score);
 switch (score / 10) {
 case 10:
 case 9: printf("합격 : 학점 A\n"); break;
 case 8: printf("합격 : 학점 B\n"); break;
 case 7: printf("합격 : 학점 C\n"); break;
 case 6: printf("합격 : 학점 D\n"); break;
 default: printf("불합격 : 학점 F\n"); break;
 }
}
